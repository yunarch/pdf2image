  },
  "description": "Application to convert a pdf into image and vice versa",
  "repository": {
    "type": "git",
    "url": "https://github.com/yunarch/pdf2image"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "husky install",
    "lint": "next lint",
    "lint:ts": "tsc --noEmit",
    "format": "prettier --check \"**/*.{js,ts,tsx,md,mdx,json}\" .",
    "format:all": "prettier --write \"**/*.{js,ts,tsx,md,mdx,json}\" .",
    "clean": "rimraf dist build out .next tsconfig.tsbuildinfo",
    "dev": "next dev -p 3000"
